{
  "name": "hny-ui",
  "version": "1.1.4",
  "description": "hny-ui 统一组件库 - 基于 Vue 3 的现代化组件库",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.cjs",
      "types": "./dist/components/index.d.ts"
    },
    "./directive": {
      "import": "./dist/directive/index.mjs",
      "require": "./dist/directive/index.cjs",
      "types": "./dist/directive/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.mjs",
      "require": "./dist/hooks/index.cjs",
      "types": "./dist/hooks/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.cjs",
      "types": "./dist/utils/index.d.ts"
    },
    "./styles": {
      "import": "./dist/styles/index.css",
      "require": "./dist/styles/index.css"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "clean": "rimraf dist"
  },
  "keywords": [
    "vue3",
    "component library",
    "ui framework",
    "typescript",
    "packages"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "element-plus": "^2.11.5",
    "vue": "^3.3.0",
    "axios": "^1.13.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "glob": "^11.0.0",
    "rimraf": "^6.0.1",
    "sass": "^1.93.2",
    "typescript": "^5.6.3",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.2.1",
    "vue-tsc": "^2.2.12"
  },
  "dependencies": {
    "axios": "^1.13.1"
  }
}
